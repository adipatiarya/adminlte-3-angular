{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "adminlte": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "",
            "sourceRoot": "src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@ngx-env/builder:application",
                    "options": {
                        "outputPath": {
                            "base": "dist/adminlte"
                        },
                        "ngxEnv": {
                            "prefix": "NG_APP_",
                            "runtime": true,
                            "unsecure": true
                        },
                        "index": "src/index.html",
                        "polyfills": ["src/polyfills.ts"],
                        "tsConfig": "tsconfig.app.json",
                        "assets": ["src/favicon.ico", "src/assets"],
                        "styles": [
                            "src/styles.scss",
                            "node_modules/ngx-toastr/toastr.css"
                        ],
                        "scripts": [],
                        "preserveSymlinks": true,
                        "extractLicenses": false,
                        "sourceMap": true,
                        "optimization": false,
                        "namedChunks": true,
                        "allowedCommonJsDependencies": ["path", "os", "fs"],
                        "browser": "src/main.ts"
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@ngx-env/builder:dev-server",
                    "options": {
                        "buildTarget": "adminlte:build",
                        "ngxEnv": {
                            "prefix": "NG_APP_",
                            "runtime": true
                        }
                    },
                    "configurations": {
                        "production": {
                            "buildTarget": "adminlte:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@ngx-env/builder:extract-i18n",
                    "options": {
                        "buildTarget": "adminlte:build"
                    }
                },
                "test": {
                    "builder": "@ngx-env/builder:karma",
                    "options": {
                        "main": "src/test.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "tsconfig.spec.json",
                        "karmaConfig": "karma.conf.js",
                        "assets": ["src/favicon.ico", "src/assets"],
                        "styles": [
                            "src/styles.scss",
                            "node_modules/ngx-toastr/toastr.css"
                        ],
                        "scripts": []
                    }
                },
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "lintFilePatterns": ["src/**/*.ts", "src/**/*.html"]
                    }
                },
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "e2e/protractor.conf.js",
                        "devServerTarget": "adminlte:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "adminlte:serve:production"
                        }
                    }
                }
            }
        }
    },
    "cli": {
        "analytics": "43a7ce40-5fb9-4a0d-8b40-b48fc0426e9d",
        "schematicCollections": ["@angular-eslint/schematics"]
    }
}
